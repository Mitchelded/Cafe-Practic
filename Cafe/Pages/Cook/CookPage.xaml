<Page x:Class="Cafe.Pages.Cook.CookPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Cafe.Pages.Cook"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CookPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="15*"/>
            <RowDefinition Height="2*"/>
        </Grid.RowDefinitions>
        <Frame Name="frameDG" NavigationUIVisibility="Hidden" Margin="10, 25,10,10" Grid.Row="0"/>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom" Grid.Row="1">
            <Button  Content="Show All Not Taking" x:Name="ShowAllOrder" Click="ShowAllOrder_Click" Margin="10"/>
            <Button  Content="Show All Taking" x:Name="ShowOrder" Click="ShowOrder_Click" Margin="10"/>
        </StackPanel>

    </Grid>
</Page>
